programa {
  funcao inicio() {
   //Estrutura de repetição for (para)
   /*
   (outra forma de fazer comentário)
   incremento   /   decremento
   i=i+1=>i++       i=i-1=>i--
   */
   
    //Contando de 1 a 10
    para(inteiro i=1; i<=10; i++)
    escreva (i, " ")
    escreva("\n")
    //Contando de 10 a 1
    para(inteiro i=10; i>=1; i--)
    escreva ( i, " ")
  }
}
